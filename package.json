{
	"name": "livegraphsdjango",
	"version": "0.1.0",
	"description": "Live Graphs Django Dashboard",
	"private": true,
	"scripts": {
		"lint": "eslint dashboard_project/static/js/",
		"format": "prettier --write \"dashboard_project/static/**/*.{js,css,html}\"",
